@model List <Project.Models.StudentInfo>

@{
    ViewBag.Title = "Submit";
}



<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>


    <title>My First Practise on BootsTrap</title>
    <meta name="viewport" content="width=device-width,initial-scale=1.0"/>

    <<link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet" media="all"/>
    
    <link href="~/Content/style2.css" rel="stylesheet" media="all"/>
    <link type="text/css" rel="stylesheet" href="~/Content/style3.css"  />
    <link type="text/css" rel="stylesheet" href="~/Content/style4.css" media="only screen and (max-width: 540px)" />
    <!-- Custom styles for this template -->
   
    </head>

<h2>Submitted :)</h2>
    <body>

<table data-toggle="table" data-url="data1.json" data-cache="false" data-height="299">
    <thead>
        <tr>
            <th data-field="id">FirstName</th>
             <th data-field="id">LastName</th>
             <th data-field="id">FatherName</th>
             <th data-field="id">Ocupation</th>
             <th data-field="id">Nationality</th>
             <th data-field="id">Religion</th>
             <th data-field="id">Nic</th>
             <th data-field="id">Domicile</th>
             <th data-field="id">Address</th>
             <th data-field="id">Province</th>
             <th data-field="id">City</th>
             <th data-field="id">Date of Birth</th>
             <th data-field="id">Gendre</th>
             <th data-field="id">Hafiz</th>
             <th data-field="id">Phone</th>
             <th data-field="id">Mobile</th>
            <th data-field="id">image</th>




           
        </tr>
    </thead>

     @foreach (var d in Model)
    { 
       <tbody>
    <tr id="tr_id_1" class="tr-class-1">
        <td id="td_id_1" class="td-class-1">1</td>
        <td>@d.FirstName</td>
        <td>@d.LastName</td>
        <td>@d.FatherName</td>
        <td>@d.Occupation</td>
        <td>@d.Nationality</td>
        <td>@d.Religion</td>
        <td>@d.Nic</td>
        <td>@d.Domicile</td>
        <td>@d.Address</td>
        <td>@d.Province</td>
        <td>@d.City</td>
        <td>@d.Dob</td>
        <td>@d.Gender</td>
        <td>@d.Hafiz</td>
        <td>@d.Phone</td>
        <td>@d.Mobile</td>
         <td style="height:200px;width:200px"><img src="~/Files/@d.Image" style="height:150px;width:150px;"/></td>

        
    </tr>
           </tbody>
        
    }
    </table>

        </body>
    </html>
