  <body>
      <div style="padding-left:800px">
          <form action="/AdminSign/AdminSign" method="post">
              <button class="btn btn-lg btn-primary" type="submit" >Sign up</button> 
              </form>

      </div>
    <div class="container">

      <form class="form-signin" action="/Admin/Verify" method="post">
        <h2 class="form-signin-heading">Admin Panel</h2>
        <label for="inputEmail" class="sr-only">Email address</label>
        <input type="text" id="inputEmail" name="Name" class="form-control" placeholder="Email address" required autofocus>
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" name="Password" class="form-control" placeholder="Password" required>
        <!--<div class="checkbox">-->
         <!-- <label>
